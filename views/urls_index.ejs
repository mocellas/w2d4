<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
<header></header>
    <!-- Add your EJS code here -->
    <table>
     <thead>
      <tr>
        <th>Short</th><th>Long</th>
      </tr>
      </thead>
      <tbody>
        <% for(var urlKey in urls) { %>
        <form method="POST" action="/urls/<%= urlKey %>/delete">
          <tr>
            <td><%= urlKey %></td>
            <td><%= urls[urlKey].URL%></td>
            <td><a href="/urls/<%= urlKey %>"> Edit </a></td>
            <td><input type="submit" value="delete"></td>
          </tr>
         </form>
        <% } %>
      </tbody>
    </table>
    <a href="/urls/new">Create a new link</a>
</body>
</html>